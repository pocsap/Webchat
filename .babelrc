{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": {
          "node": "current",
        },
        "useBuiltIns": "entry"
      },
    ],
    "@babel/preset-react",
  ],
  "plugins": [
    "@babel/plugin-proposal-function-bind",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
  ],
}
